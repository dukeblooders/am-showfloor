class Game
{
	name = null
	title = null
	manufacturer = null
	extra = null
	
	constructor(_name, _title, _manufacturer, _extra)
	{
		name = _name
		title = _title
		manufacturer = _manufacturer
		extra = _extra
	}
}

class Rectangle
{
	x = null; y = null; width = null; height = null
	
	constructor(_x, _y, _width, _height)
	{
		x = _x
		y = _y
		width = _width
		height = _height
	}
}